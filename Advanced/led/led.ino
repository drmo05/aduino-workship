void setup() {
  pinMode(3, OUTPUT);
}
void loop() {
  analogWrite(3, 255);
  delay(1000);
  analogWrite(3, 205);
  delay(1000);
  analogWrite(3, 155);
  delay(1000);
  analogWrite(3, 105);
  delay(1000);
  analogWrite(3, 55);
  delay(1000);
  analogWrite(3, 5);
  delay(1000);
  analogWrite(3, 0);
  delay(1000);
  analogWrite(3, 5);
  delay(1000);
  analogWrite(3, 55);
  delay(1000);
  analogWrite(3, 105);
  delay(1000);
  analogWrite(3, 155);
  delay(1000);
  analogWrite(3, 205);
  delay(1000);
}
